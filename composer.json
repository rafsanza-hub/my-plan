{
    "name": "rafsanza-hub/my-plan",
    "description": "Web pengelolaan keuangan pribadi.",
    "license": "GPL-3.0",
    "type": "project",
    "homepage": "https://github.com/rafsanza-hub/my-plan",
    "support": {
        "source": "https://github.com/rafsanza-hub/my-plan"
    },
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.0",
        "myth/auth": "^1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.5.16"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
